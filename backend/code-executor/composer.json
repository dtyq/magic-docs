{
  "name": "dtyq/code-executor",
  "description": "A multi-language code execution system",
  "type": "library",
  "license": "MIT",
  "keywords": [],
  "require": {
    "ext-zip": "*",
    "alibabacloud/fc-20230330": "^4.1",
    "hyperf/codec": "^3.1",
    "hyperf/di": "^3.1",
    "hyperf/support": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.9",
    "mockery/mockery": "^1.5",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "files": [
      "src/Utils/Functions.php"
    ],
    "psr-4": {
      "Dtyq\\CodeExecutor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dtyq\\CodeExecutor\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "hyperf": {
      "config": "Dtyq\\CodeExecutor\\ConfigProvider"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "cs-fix": "php-cs-fixer fix",
    "phpstan": "phpstan analyse"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
